@page
@model JurayKV.UI.Areas.KvMain.Pages.IUsers.IndexDataModel
@{
}

<div class="card">
    <div class="card-header">
        <h3 class="card-title">@Model.Title</h3>

        <a asp-page="./UserList" class="btn bg-gradient-warning btn-sm" style="float:right;">Users List JQuery</a>

    </div>
    <div class="card-header">
        <h6>Duplicate Phone Manager</h6>

        <div class="">
            @Model.DistintPhone
        </div>
    </div>
    <div class="card-header">
        <a asp-page="./Index" asp-route-all="true" class="btn btn-primary">
            All Users (@Model.All)
        </a>

        <a asp-page="./Index" asp-route-status="@JurayKV.Domain.Primitives.Enum.AccountStatus.Active" asp-route-startdate="@DateTime.MinValue" asp-route-enddate="@DateTime.MaxValue" asp-route-searchstring="@Model.Searchstring" class="btn btn-primary">
            Active Users (@Model.ActiveOnly)
        </a>

        <a asp-page="./Index" asp-route-status="@JurayKV.Domain.Primitives.Enum.AccountStatus.Suspended" asp-route-startdate="@DateTime.MinValue" asp-route-enddate="@DateTime.MaxValue" asp-route-searchstring="@Model.Searchstring" class="btn btn-danger">
            Suspended Users (@Model.Suspended)
        </a>
        <a asp-page="./Index" asp-route-status="@JurayKV.Domain.Primitives.Enum.AccountStatus.Disabled" asp-route-startdate="@DateTime.MinValue" asp-route-enddate="@DateTime.MaxValue" asp-route-searchstring="@Model.Searchstring" class="btn btn-warning">
            Disabled Users (@Model.Disabled)
        </a>
    </div>
    <div class="card-header">
        @if (Model.Status == JurayKV.Domain.Primitives.Enum.AccountStatus.NotDefind && Model.Searchstring == null)
        {
            <h6>CURRENT PAGE STATISTICS</h6>
        }
        else if (Model.Searchstring != null)
        {
            <h6>@Model.Searchstring.ToUpper() CURRENT PAGE  STATISTICS</h6>
        }
        else if (Model.StartDate != null)
        {
            <h6>Update from <span class="bg-info btn">@Model.StartDate</span> to <span class="bg-info btn">@Model.EndDate</span> CURRENT PAGE STATISTICS</h6>

        }
        <a asp-page="./Index" asp-route-all="true" class="btn btn-primary">
            Dated Users (@Model.DayAll)
        </a>

        <a asp-page="./Index" asp-route-status="@JurayKV.Domain.Primitives.Enum.AccountStatus.Active" asp-route-startdate="@Model.StartDate" asp-route-enddate="@Model.EndDate" asp-route-searchstring="@Model.Searchstring" class="btn btn-primary">
            Dated Active Users (@Model.DayActiveOnly)
        </a>

        <a asp-page="./Index" asp-route-status="@JurayKV.Domain.Primitives.Enum.AccountStatus.Suspended" asp-route-startdate="@Model.StartDate" asp-route-enddate="@Model.EndDate" asp-route-searchstring="@Model.Searchstring" class="btn btn-danger">
            Dated Suspended Users (@Model.DaySuspended)
        </a>
        <a asp-page="./Index" asp-route-status="@JurayKV.Domain.Primitives.Enum.AccountStatus.Disabled" asp-route-startdate="@Model.StartDate" asp-route-enddate="@Model.EndDate" asp-route-searchstring="@Model.Searchstring" class="btn btn-warning">
            Dated Disabled Users (@Model.DayDisabled)
        </a>
    </div>
    <div class="card-header">
        <form class="">
            <div class="row">

                <div class="form-group col-md-3">
                    <label for="searchBox" class="">Search:</label>
                    <input type="text" name="searchstring" class="form-control" id="searchBox" value="@Model.Searchstring" placeholder="Enter search term">
                </div>
                <div class="form-group col-md-3">
                    <label for="startDate" class="">Start Date:</label>
                    <input type="date" name="startdate" class="form-control" id="startDate" value="@Model.StartDate">
                </div>
                <div class="form-group col-md-3">
                    <label for="endDate" class="">End Date:</label>
                    <input type="date" name="enddate" class="form-control" id="endDate" value="@Model.EndDate">
                </div>
                <div class="form-group col-md-3">

                    <button type="submit" class="btn btn-primary">Search</button>
                </div>
            </div>
        </form>
    </div>
