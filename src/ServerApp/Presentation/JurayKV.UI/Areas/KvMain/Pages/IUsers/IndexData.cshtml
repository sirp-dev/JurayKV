        <div class="pagination">
            @if (Model.PageNumber > 1)
            {
                @if (Model.ShowAll == true || Model.ShowWithoutDate == true)
                {
                    <a asp-page="./Index" asp-route-pageNumber="@(Model.PageNumber - 1)" asp-route-pageSize="@Model.PageSize" asp-route-status="@Model.Status" asp-route-searchstring="@Model.Searchstring" class="">Previous</a>
                }
                else
                {
                    <a asp-page="./Index" asp-route-pageNumber="@(Model.PageNumber - 1)" asp-route-pageSize="@Model.PageSize" asp-route-status="@Model.Status" asp-route-startdate="@Model.StartDate" asp-route-enddate="@Model.EndDate" asp-route-searchstring="@Model.Searchstring" class="">Previous</a>
                }
            }

            @for (int i = 1; i <= Model.TotalPages; i++)
            {
                if (i == (Model.PageNumber - 2) && i != (Model.TotalPages - 2))
                {
                    if (Model.ShowAll == true || Model.ShowWithoutDate == true)
                    {
                        <a asp-page="./Index" asp-route-pageNumber="@i" asp-route-pageSize="@Model.PageSize" asp-route-status="@Model.Status" asp-route-searchstring="@Model.Searchstring" class="@(i == Model.PageNumber ? "active" : "")">@i</a>
                    }
                    else
                    {
                        <a asp-page="./Index" asp-route-pageNumber="@i" asp-route-pageSize="@Model.PageSize" asp-route-status="@Model.Status" asp-route-startdate="@Model.StartDate" asp-route-enddate="@Model.EndDate" asp-route-searchstring="@Model.Searchstring" class="@(i == Model.PageNumber ? "active" : "")">@i</a>
