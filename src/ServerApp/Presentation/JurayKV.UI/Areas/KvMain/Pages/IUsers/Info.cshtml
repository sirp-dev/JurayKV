        <div>
            <div class="card card-success">
                <div class="card-header">
                    <h3 class="card-title">REFERRALS </h3>

                </div>
                <div class="card-body">
                    <h6>User Referral Count: @Model.UpdateUserManager.TotalReferrals</h6>
                    <h6>Referred By @Model.UpdateUserManager.UserManagerDetailsDto.RefferedBy</h6>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table id="" class="table table-bordered table-striped">
                                <thead>
                                    <tr>
                                        <th>S/N</th>
                                        <th>
                                            Fullname
                                        </th>
                                        <th>Phone Number </th>
                                        <th>Date </th>
                                        <th>Last LoggedIn </th>
                                        <th>Verified </th>
                                        <th>Posted </th>
                                        <th>Video Upload </th>
                                        <th>Success </th>
                                    </tr>
                                </thead>
                                @{
                                    int sn = 0;
                                }
                                <tbody>
                                    @foreach (var item in Model.UpdateUserManager.LastTenReferrals)
                                    {
                                        <tr>
                                            <td>
                                                @{
                                                    sn++;
                                                }
                                                @sn
                                            </td>
                                            <td>
                                                <a asp-page="./Info" asp-route-id="@item.Id">
                                                    @Html.DisplayFor(modelItem => item.Fullname)
                                                </a>
                                            </td>

                                            <td>
                                                @Html.DisplayFor(modelItem => item.PhoneNumber)
                                            </td>
                                            <td>
                                                @item.CreationUTC.ToString("dd/MM/yyyy")
                                            </td>
                                            <td>
                                                @item.LastLoggedInAtUtc
                                            </td>

                                            <td>
                                                @if (item.Verified == true)
                                                {


                                                    <span class="btn btn-success btn-xs"><i class="fa fa-check"></i></span>
                                                }
                                                else
                                                {
                                                    <span class="btn btn-danger btn-xs"><i class="fa fa-times"></i></span>

                                                }
                                            </td>
                                            <td>
                                                @if (item.Posted == true)
                                                {


                                                    <span class="btn btn-success btn-xs"><i class="fa fa-check"></i></span>
                                                }
                                                else
                                                {
                                                    <span class="btn btn-danger btn-xs"><i class="fa fa-times"></i></span>

                                                }
                                            </td>
                                            <td>
                                                @if (item.VideoUpload == true)
                                                {


                                                    <span class="btn btn-success btn-xs"><i class="fa fa-check"></i></span>
                                                }
                                                else
                                                {
                                                    <span class="btn btn-danger btn-xs"><i class="fa fa-times"></i></span>

                                                }
                                            </td>
                                            <td>
                                                @if (item.SuccessPoint == true)
                                                {


                                                    <span class="btn btn-success btn-xs"><i class="fa fa-check"></i></span>
                                                }
                                                else
                                                {
                                                    <span class="btn btn-danger btn-xs"><i class="fa fa-times"></i></span>

                                                }
                                            </td>

                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <div class="card card-success">
                <div class="card-header">
                    <h3 class="card-title">POINTS</h3>

                </div>
                <div class="card-body">
                    <h6>How Many Time Credited Points (@Model.UpdateUserManager.PointCount times)</h6>
                    <h6>Total Point Summation (@Model.UpdateUserManager.PointSum points)</h6>
                    <h6>Total Post Made (@Model.UpdateUserManager.PostingCount)</h6>
                    <h6>Total Completed Advert Post (Video Evidence) (@Model.UpdateUserManager.UploadCount)</h6>
                    <div class="card-body p-0">
                        <div class="table-responsive">
                            <table class="table m-0">
                                <thead>
                                    <tr>
                                        <th>S/N</th>
                                        <th>Date</th>
                                        <th>Point</th>
                                        <th>Status</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                @{ int xsn = 0;}
                                <tbody>
                                    @if(Model.UpdateUserManager.LastTenPoints != null) { 
                                    @foreach (var item in Model.UpdateUserManager.LastTenPoints)
                                    {
                                        <tr>
                                            @{
                                                xsn++;
                                            }
                                            <td>@xsn</td>
                                            <td><a href="">@item.CreatedAtUtc.ToString("dd/MM/yyyy hh:mm tt")</a></td>
                                            <td>
                                                @Html.DisplayFor(model => item.Point)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(model => item.Status)
                                            </td>
                                            <td>
                                                <a class="btn btn-warning" asp-page="./Details" asp-route-id="@item.IdentityKvAdId">
                                                    Details
                                                </a>
                                            </td>
                                        </tr>
                                    }
                                                }
                                </tbody>
                            </table>
                        </div>
                        <!-- /.table-responsive -->
                    </div>
                </div>
            </div>
            <div class="card card-success">
                <div class="card-header">
                    <h3 class="card-title">TRANSACTIONS (Latest 30)</h3>

                </div>
                <div class="card-body">
                    <h6>Total Credit Count @Model.UpdateUserManager.TransactionCreditCount</h6>
                    <h6>Total Debit Count @Model.UpdateUserManager.TransactionDebitCount</h6>
                </div>
                <div class="card-body">
                    <h5>CREDIT BREAKDOWN</h5>
                    @if(Model.UpdateUserManager.GroupByDescriptionCredit != null) { 
                    @foreach (var cdown in Model.UpdateUserManager.GroupByDescriptionCredit)
                    {
                        <h4>@cdown.Description</h4>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table id="example1" class="table table-bordered table-striped">
                                    <thead>
                                        <tr>
                                            <th>S/N</th>
                                            <th>
                                                Date
                                            </th>

                                            <th>Amount </th>

                                            <th>Note </th>
                                            <th>Type </th>
                                            <th>Status </th>
                                            <th>Action</th>

                                        </tr>
                                    </thead>
                                    @{
                                        int csn = 0;
                                    }
                                    <tbody>
                                        @foreach (var item in cdown.ListTransactions)
                                        {
                                            <tr>
                                                <td>
                                                    @{
                                                        csn++;
                                                    }
                                                    @csn
                                                </td>

                                                <td>
                                                    <a asp-page="./TransactionDetails" asp-route-id="@item.Id">
                                                        @item.CreatedAtUtc.ToString("dd/MM/yyyy")
                                                    </a>
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.Amount)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.UniqueReference)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.TransactionType)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.Status)
                                                </td>
                                                <td>
                                                    <a class="btn btn-primary" asp-page="./TransactionInfo" asp-route-id="@item.Id">
                                                        More
                                                    </a>
                                                </td>


                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>

                        <hr />
                    }
                                                        }  </div>

                <div class="card-body">
                    <h5>DEBIT BREAKDOWN</h5>
                    @if(Model.UpdateUserManager.GroupByDescriptionDebit != null) { 
                    @foreach (var cdown in Model.UpdateUserManager.GroupByDescriptionDebit)
                    {
                        <h4>@cdown.Description</h4>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table id="example1" class="table table-bordered table-striped">
                                    <thead>
                                        <tr>
                                            <th>S/N</th>
                                            <th>
                                                Date
                                            </th>

                                            <th>Amount </th>

                                            <th>Note </th>
                                            <th>Type </th>
                                            <th>Status </th>
                                            <th>Action</th>

                                        </tr>
                                    </thead>
                                    @{
                                        int csn = 0;
                                    }
                                    <tbody>
                                        @foreach (var item in cdown.ListTransactions)
                                        {
                                            <tr>
                                                <td>
                                                    @{
                                                        csn++;
                                                    }
                                                    @csn
                                                </td>

                                                <td>
                                                    <a asp-page="./TransactionDetails" asp-route-id="@item.Id">
                                                        @item.CreatedAtUtc.ToString("dd/MM/yyyy")
                                                    </a>
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.Amount)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.UniqueReference)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.TransactionType)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.Status)
                                                </td>
                                                <td>
                                                    <a class="btn btn-primary" asp-page="./TransactionInfo" asp-route-id="@item.Id">
                                                        More
                                                    </a>
                                                </td>


                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    }
                                                        }    </div>
            </div>
        </div>
    </div>
    <div class="card-footer">
        <a class="btn btn-warning" asp-page="./Update" asp-route-id="@Model.UpdateUserManager.UserManagerDetailsDto.Id">Update</a>
        @if (User.IsInRole(JurayKV.Application.Constants.SuperAdminPolicy) || User.IsInRole(JurayKV.Application.Constants.ValidatorPolicy))
        {


            <a class="btn btn-warning" asp-page="./ValidateTransaction" asp-route-userId="@Model.UpdateUserManager.UserManagerDetailsDto.Id">Validate Transaction</a>
        }
        @if (User.IsInRole(JurayKV.Application.Constants.SuperAdminPolicy) || User.IsInRole(JurayKV.Application.Constants.Permission))
        {
            <a class="btn btn-warning" asp-page="/IPermissions/UserRoles" asp-route-id="@Model.UpdateUserManager.UserManagerDetailsDto.Id">Manage Roles</a>
            <a class="btn btn-danger" asp-page="./Remove" asp-route-id="@Model.UpdateUserManager.UserManagerDetailsDto.Id">Remove account</a>
        }
    </div>
</div>


<!-- Trigger the modal with a button -->
<!-- Modal -->
<div id="projectDescription" class="modal fade" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">LOGs</h4>
            </div>
            <div class="modal-body" style="font-size:10px;text-align:justify;">
                <ul>
                    @if(Model.UpdateUserManager.WalletDetailsDto != null) { 
                    @Html.Raw(Model.UpdateUserManager.WalletDetailsDto.Log)
                    }
                </ul>

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>

    </div>
</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
